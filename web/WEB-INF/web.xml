<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>controller.RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddRegistration</servlet-name>
        <servlet-class>controller.AddRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteRegistration</servlet-name>
        <servlet-class>controller.DeleteRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateRegistrations</servlet-name>
        <servlet-class>controller.UpdateRegistrations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilterRegistration</servlet-name>
        <servlet-class>controller.FilterRegistration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/Regis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddRegistration</servlet-name>
        <url-pattern>/AddRegis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteRegistration</servlet-name>
        <url-pattern>/DelRegis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateRegistrations</servlet-name>
        <url-pattern>/UpdateRegis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilterRegistration</servlet-name>
        <url-pattern>/Filter</url-pattern>
    </servlet-mapping>
</web-app>
